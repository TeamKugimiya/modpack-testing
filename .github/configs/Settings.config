# 模組包名稱
ModPackName=YEE-Test

# 是否生成「模組包資源包補丁」
Resourcepack_Patch_Generate=true

# 補丁複製陣列（哪些資料夾與檔案需要複製進補丁）
Patch_Array=(config kubejs resourcepacks LICENSE)

# 伺服器複製陣列
Server_Array(config/server_only kubejs/server_script LICENSE)

# 是否生成「伺服器補丁」
Server_Patch_Generate=true

# 是否自動下載「簡轉繁資源包」
Download_MMLP=true